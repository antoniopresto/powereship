{
  "name": "@powership/utils",
  "version": "2.3.1",
  "typings": "out",
  "author": "antoniopresto <antoniopresto@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "./out/index.cjs",
  "module": "./out/module/index.js",
  "types": "./out/index.d.ts",
  "exports": {
    "types": "./out/index.d.ts",
    "import": "./out/module/index.js",
    "require": "./out/index.js"
  },
  "browser": {
    "out/module/index.js": "./out/browser/module/index.js",
    "out/index.js": "./out/browser/index.js"
  },
  "scripts": {
    "test": "jest",
    "build": "run-s clear && run-p declarations build:*",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "clear": "rimraf out",
    "prepublishOnly": "run-s build fix && run-p test",
    "build-targets": "run-p build:*",
    "build:browser": "TARGET=browser npm run babild -- --out-dir out/browser",
    "build:module-browser": "TARGET=module-browser npm run babild -- --out-dir out/browser/module",
    "build:node": "TARGET=node npm run babild -- --out-dir out",
    "build:module-node": "TARGET=module-node npm run babild -- --out-dir out/module",
    "declarations": "tsc -p tsconfig.json",
    "babild": "babel 'src' --extensions '.ts,.tsx' --source-maps=true --ignore '**/__tests__'"
  },
  "description": "powership utils and helper functions",
  "keywords": [],
  "dependencies": {
    "aggio": "0.2.0",
    "awesome-phonenumber": "4.1.0",
    "big.js": "6.2.1",
    "chalk": "4.1.2",
    "dayjs": "1.11.6",
    "deep-diff": "1.0.2",
    "deep-object-diff": "1.1.9",
    "ejson": "2.2.3",
    "fast-copy": "3.0.0",
    "fast-deep-equal": "3.1.3",
    "fs-extra": "10.1.0",
    "graphql": "16.6.0",
    "graphql-parse-resolve-info": "4.13.0",
    "hoper": "1.0.8",
    "immer": "10.0.2",
    "json-schema-to-typescript": "11.0.2",
    "jsondiffpatch": "^0.4.1",
    "lodash": "4.17.21",
    "mitt": "3.0.0",
    "object-hash": "3.0.0",
    "plugin-hooks": "2.0.0",
    "prettier": "2.8.8",
    "slugify": "1.6.5",
    "ts-toolbelt": "9.6.0",
    "ulid": "2.3.0"
  },
  "devDependencies": {
    "@powership/boilerplate": "workspace:*",
    "@babel/cli": "7.19.3",
    "@babel/plugin-transform-typescript": "7.19.3",
    "@babel/preset-env": "7.19.3",
    "@babel/preset-typescript": "7.18.6",
    "@powership/babel-plugins": "workspace:*",
    "@types/big.js": "6.1.6",
    "@types/deep-diff": "^1.0.2",
    "@types/ejson": "2.2.0",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "29.5.3",
    "@types/json-schema": "7.0.11",
    "@types/lodash": "4.14.191",
    "@types/node": "16.18.3",
    "@types/object-hash": "2.2.1",
    "@types/prettier": "2.7.1",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "babel-preset-minify": "0.5.2",
    "esbuild": "0.15.10",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "jest": "29.6.2",
    "jest-diff": "29.1.2",
    "mongodb": "4.10.0",
    "npm-run-all": "4.1.5",
    "prettier-plugin-multiline-arrays": "^1.1.3",
    "rimraf": "3.0.2",
    "ts-jest": "29.1.0",
    "typedoc": "^0.23.24",
    "typescript": "4.9.3"
  },
  "files": [
    "package.json",
    "out/*",
    "README.md"
  ],
  "prettier": {
    "singleQuote": true
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "Utils - A collection of common utilities. Internal or from other libraries",
    "tsconfig": "./tsconfig.module.json"
  }
}
