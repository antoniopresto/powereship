{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "prepublishOnly": {
      "dependsOn": ["^build"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "lib/**", "public/dist/**"]
    },
    "fix": {},
    "test": {},
    "clear": {},
    "dev": {},
    "declarations": {
      "dependsOn": ["^declarations"],
      "outputs": ["dist/**", "lib/**", ".next/**", "public/dist/**"]
    }
  }
}
