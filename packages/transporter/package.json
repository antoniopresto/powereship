{
  "name": "@backland/transporter",
  "version": "0.1.3-alpha.2022.11.30.23.08.37.0",
  "main": "lib/index.js",
  "sideEffects": false,
  "module": "lib/module/index.js",
  "author": "antoniopresto <antoniopresto@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "run-s clear && run-p declarations build:* && sleep 3",
    "clear": "rimraf lib",
    "fix": "run-s fix:* && sleep 3 ",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "prepublish": "run-s build fix && run-p test",
    "build-targets": "run-p build:*",
    "build:node": "TARGET=node npm run babild -- --out-dir lib",
    "build:module-node": "TARGET=module-node npm run babild -- --out-dir lib/module",
    "declarations": "tsc -p tsconfig.declarations.json",
    "babild": "babel 'src' --extensions '.ts,.tsx'"
  },
  "description": "Backland db",
  "repository": {
    "type": "git",
    "url": "https://github.com/antoniopresto/backland/tree/master/packages/db"
  },
  "keywords": [
    "typescript",
    "schema",
    "graphql",
    "validation",
    "type",
    "inference"
  ],
  "dependencies": {
    "@backland/schema": "0.1.3-alpha.2022.11.30.23.08.37.0",
    "@backland/utils": "0.1.3-alpha.2022.11.30.23.08.37.0",
    "dataloader": "2.1.0",
    "plugin-hooks": "1.1.3",
    "sift": "16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/plugin-transform-typescript": "7.19.3",
    "@babel/preset-env": "7.19.3",
    "@babel/preset-typescript": "7.18.6",
    "@backland/babel-plugins": "0.1.3-alpha.2022.11.30.23.08.37.0",
    "@types/jest": "29.1.2",
    "@types/node": "16.18.3",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "conditional-type-checks": "1.0.6",
    "esbuild": "0.15.10",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-eslint-comments": "3.2.0",
