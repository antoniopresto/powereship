{
  "name": "@swind/accounts",
  "version": "1.0.9",
  "description": "Solarwind accounts",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "run-s clear && run-p declarations build:*",
    "clear": "rimraf lib",
    "fix": "run-s fix:* ",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "prepublishOnly": "run-s build fix && run-p test",
    "build-targets": "run-p build:*",
    "build:node": "TARGET=node npm run babild -- --out-dir lib",
    "build:module-node": "TARGET=module-node npm run babild -- --out-dir lib/module",
    "declarations": "tsc -p tsconfig.declarations.json",
    "babild": "babel 'src' --extensions '.ts,.tsx'"
  },
  "files": [
    "package.json",
    "lib/*",
    "README.md"
  ],
  "repository": {
    "type": "git"
  },
  "author": "@antoniopresto",
  "dependencies": {
    "@swind/entity": "1.0.9",
    "@swind/schema": "1.0.9",
    "@swind/utils": "1.0.9",
    "@swind/transporter": "1.0.9",
    "jsonwebtoken": "8.5.1",
    "scrypt-kdf": "2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/plugin-transform-typescript": "7.19.3",
    "@babel/preset-env": "7.19.3",
    "@babel/preset-typescript": "7.18.6",
    "@swind/babel-plugins": "1.0.9",
    "@swind/mongo": "1.0.9",
    "@types/jest": "29.1.2",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "16.18.3",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "babel-preset-minify": "0.5.2",
    "esbuild": "0.15.10",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "jest": "29.3.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-multiline-arrays": "^1.1.3",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "typedoc": "^0.23.24",
    "typescript": "4.9.3"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "Accounts System.",
    "tsconfig": "./tsconfig.module.json"
  }
}
