{
  "name": "@backland/accounts",
  "version": "0.1.3-alpha.2022.11.30.23.08.37.0",
  "description": "Backland accounts",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test": "jest",
    "build": "run-s clear && run-p declarations build:* && sleep 3",
    "clear": "rimraf lib",
    "fix": "run-s fix:* && sleep 3 ",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "prepublish": "run-s build fix && run-p test",
    "build-targets": "run-p build:*",
    "build:node": "TARGET=node npm run babild -- --out-dir lib",
    "build:module-node": "TARGET=module-node npm run babild -- --out-dir lib/module",
    "declarations": "tsc -p tsconfig.declarations.json",
    "babild": "babel 'src' --extensions '.ts,.tsx'"
  },
  "files": [
    "package.json",
    "lib/*",
    "README.md"
  ],
  "repository": {
    "type": "git"
  },
  "author": "@antoniopresto",
  "dependencies": {
    "@backland/entity": "0.1.3-alpha.2022.11.30.23.08.37.0",
    "@backland/schema": "0.1.3-alpha.2022.11.30.23.08.37.0",
    "@backland/utils": "0.1.3-alpha.2022.11.30.23.08.37.0",
    "jsonwebtoken": "8.5.1",
    "scrypt-kdf": "2.0.1"
  },
  "devDependencies": {
