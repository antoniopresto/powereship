{
  "name": "backland",
  "version": "0.0.0-alpha.20221217015336810",
  "main": "lib/index.js",
  "module": "lib/module/index.js",
  "browser": {
    "lib/module/index.js": "./lib/browser/module/index.js",
    "lib/index.js": "./lib/browser/index.js"
  },
  "sideEffects": false,
  "author": "antoniopresto <antoniopresto@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "run-s clear declarations build:* && sleep 3",
    "clear": "rimraf lib",
    "fix": "run-s fix:* && sleep 3 ",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "prepublish": "run-s build fix && run-p test",
    "build-targets": "run-p build:*",
    "build:browser": "TARGET=browser npm run babild -- --out-dir lib/browser",
    "build:module-browser": "TARGET=module-browser npm run babild -- --out-dir lib/browser/module",
    "build:node": "TARGET=node npm run babild -- --out-dir lib",
    "declarations": "tsc -p tsconfig.declarations.json",
    "babild": "babel 'src' --extensions '.ts,.tsx' --source-maps=true --ignore '**/__tests__'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antoniopresto/backland/tree/master/packages/db"
  },
  "dependencies": {
    "@backland/accounts": "0.0.0-alpha.20221217015336810",
    "@backland/entity": "0.0.0-alpha.20221217015336810",
    "@backland/mongo": "0.0.0-alpha.20221217015336810",
    "@backland/schema": "0.0.0-alpha.20221217015336810",
    "@backland/transporter": "0.0.0-alpha.20221217015336810",
    "@backland/utils": "0.0.0-alpha.20221217015336810"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.19.3",
    "@babel/plugin-transform-typescript": "7.19.3",
    "@babel/preset-env": "7.19.3",
    "@babel/preset-typescript": "7.18.6",
    "@backland/babel-plugins": "0.0.0-alpha.20221217015336810",
    "@types/babel__core": "7.1.19",
    "@types/babel__helper-plugin-utils": "7.10.0",
    "@types/jest": "29.1.2",
    "@types/node": "16.18.3",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "babel-plugin-transform-strip-block": "0.0.5",
    "babel-preset-minify": "0.5.2",
    "conditional-type-checks": "1.0.6",
    "esbuild": "0.15.10",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "graphql": "16.6.0",
    "jest": "29.1.2",
    "mongodb": "4.10.0",
    "mongodb-memory-server-core": "8.9.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "files": [
    "package.json",
    "lib/*",
    "README.md"
  ]
