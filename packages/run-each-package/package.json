{
  "name": "run-each-package",
  "version": "0.0.0-alpha.20230110164903223",
  "typings": "lib",
  "author": "antoniopresto <antoniopresto@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/module/index.js",
  "sideEffects": false,
  "scripts": {
    "test": "echo ok",
    "build": "run-s clear && run-p declarations build:*",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "clear": "rimraf lib",
    "prepublish": "run-s build fix && run-p test",
    "build-targets": "run-p build:*",
    "build:node": "TARGET=node npm run babild -- --out-dir lib",
    "build:module-node": "TARGET=module-node npm run babild -- --out-dir lib/module",
    "declarations": "tsc -p tsconfig.json",
    "babild": "babel 'src' --extensions '.ts,.tsx' --source-maps=true --ignore '**/__tests__'"
  },
  "description": "run command in each package",
  "keywords": [],
  "dependencies": {
    "fs-extra": "^11.1.0",
    "@backland/schema": "0.0.0-alpha.20230110164903223"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/plugin-transform-typescript": "7.19.3",
    "@babel/preset-env": "7.19.3",
    "@babel/preset-typescript": "7.18.6",
    "@backland/babel-plugins": "0.0.0-alpha.20230110164903223",
    "@types/node": "16.18.3",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "babel-preset-minify": "0.5.2",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "jest": "29.1.2",
    "npm-run-all": "4.1.5",
    "ts-jest": "29.0.3",
    "typescript": "4.9.3"
  },
  "files": [
    "package.json",
    "lib/*",
    "README.md"
  ],
  "prettier": {
    "singleQuote": true
  }
}