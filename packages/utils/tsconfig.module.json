{
  "extends": "./tsconfig",
  "compilerOptions": {
    "paths": {
      "lodash": ["./node_modules/lodash-es"],
      "lodash/*": ["./node_modules/lodash-es/*"]
    },
    "target": "esnext",
    "outDir": "out/module",
    "module": "esnext",
    "noUnusedLocals": true
  },
  "exclude": ["node_modules/**"]
}
