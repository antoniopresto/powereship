{
  "name": "root",
  "private": true,
  "version": "0.2.6",
  "scripts": {
    "clean-all": "rm -rf node_modules && yarn install && lerna clean --yes && lerna bootstrap",
    "lerna": "lerna",
    "bootstrap": "lerna bootstrap",
    "fix": "lerna run fix",
    "commit": "git add -A && git commit",
    "fast": "run-s commit alpha",
    "validate": "(cd packages/schema && npm run prepublish)",
    "lp": "lerna exec \"yalc publish\"",
    "link-all": "lerna link --force-local",
    "alpha": "ts-node ./scripts/bpa.ts",
    "exec": "node --preserve-symlinks -r ts-node/register ./scripts/run-all.ts",
    "alpha-version": "linkall && ts-node ./scripts/alpha-version.ts"
  },
  "devDependencies": {
    "@backland/babel-plugins": "0.2.4",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "29.1.2",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "16.18.3",
    "@types/shelljs": "0.8.11",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "conditional-type-checks": "1.0.6",
    "esbuild": "0.15.10",
    "eslint": "7.20.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "fs-extra": "10.1.0",
    "jest": "29.1.2",
    "lerna": "^6.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.0",
    "prettier-plugin-multiline-arrays": "^1.1.3",
    "rimraf": "3.0.2",
    "runmate": "latest",
    "shelljs": "0.8.5",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.2"
  }
}
