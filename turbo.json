{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "prepublishOnly": {
      "dependsOn": ["^prepublishOnly"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["out/**"]
    },
    "fix": {},
    "test": {},
    "clear": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "declarations": {
      "dependsOn": ["^declarations"],
      "outputs": ["out/**"]
    }
  }
}
